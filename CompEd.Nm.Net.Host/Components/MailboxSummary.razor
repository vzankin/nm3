<RadzenText TextStyle=TextStyle.H6 Text=@(Mailbox.Name) class="rz-color-info"/>
<RadzenText TextStyle=TextStyle.Caption Text=@($"{L["Last check"]}: {Cache.Info.OrderBy(x => x.Id).FirstOrDefault()?.LastCheck.ToLocalTime()}")/>

@code
